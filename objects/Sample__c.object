    </validationRules>
    <validationRules>
        <fullName>Require_feedback_to_close</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Feedback__c ) &amp;&amp; ISPICKVAL(Stage__c, &quot;Complete&quot;)</errorConditionFormula>
        <errorMessage>Cannot mark as closed without entering feedback</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Approval_required</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(Approved_by_Sales_Manager__c) &amp;&amp; 
(Stage_number_formula__c &gt; 4 || NOT(processbypass__c))
&amp;&amp;  $User.Id = Account_Manager__r.Id</errorConditionFormula>
        <errorMessage>Cannot advance past this stage without Sales Manager approval</errorMessage>
    </validationRules>
