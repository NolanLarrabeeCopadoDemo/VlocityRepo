    <fields>
        <fullName>Effectiveness_Rating__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Campaign_ROI__c &lt; 1, IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_340))+ &apos;resource/1250029193000/dollar_zero&apos;, &apos;zero&apos;) , 
IF(Campaign_ROI__c &lt; 1.5, IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_340))+ &apos;resource/1250029230000/dollar_one&apos;, &apos;one&apos;), 
IF(Campaign_ROI__c &lt; 2.0, IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_340))+ &apos;resource/1250029263000/dollar_two&apos;, &apos;two&apos;) , 
IF(Campaign_ROI__c &lt; 2.5, IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_340))+ &apos;resource/1250029415000/dollar_three&apos;, &apos;three&apos;) , 
IF(Campaign_ROI__c &lt; 3, IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_340))+ &apos;resource/1250029446000/dollar_four&apos;, &apos;four&apos;) , 
IMAGE(LEFT($Api.Partner_Server_URL_260, FIND( &apos;/services&apos;, $Api.Partner_Server_URL_340))+ &apos;resource/1250029472000/dollar_five&apos;, &apos;five&apos;) ) ) ) ) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Campaign Effectiveness is driven by the ROI.  5 Dollars signs is over 300%.</inlineHelpText>
        <label>Campaign Effectiveness</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
