    <fields>
        <fullName>Campaign_ROI__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ActualCost &gt; 0, (AmountWonOpportunities - ActualCost )/ ActualCost, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>ROI is Net Gain. (Total Amount Won - Actual Cost) / Actual Cost</inlineHelpText>
        <label>Campaign ROI</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Percent</type>
    </fields>
