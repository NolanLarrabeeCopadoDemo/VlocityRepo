      "Percent_change_vs_pr_1" : {
        "broadcastFacet" : true,
        "datasets" : [
          {
            "name" : "${App.Datasets.OpptyProduct_tp.Name}"
          }
        ],
        "isGlobal" : false,
        "label" : "Percent_change_vs_prev_30_days",
        "query" : {
          "query" : {
            "measures" : [
              [
                "avg",
                "Opportunity.Opportunity_Age",
                "Last30Days"
              ],
              [
                "avg",
                "Opportunity.Opportunity_Age",
                "Prev30Days"
              ],
              [
                "count",
                "*",
                "PercChangeVsPrev30Days"
              ],
              [
                "count",
                "*",
                "Color"
              ]
            ],
            "columns" : [
              {
                "query" : {
                  "measures" : [
                    [
                      "avg",
                      "Opportunity.Opportunity_Age"
                    ]
                  ],
                  "filters" : [
                    [
                      "Opportunity.CloseDate",
                      [
                        [
                          [
                            "day",
                            -30
                          ],
                          [
                            "day",
                            0
                          ]
                        ]
                      ],
                      ">=<="
                    ]
                  ]
                },
                "name" : "Last30Days",
                "header" : "Last30Days"
              },
              {
                "query" : {
                  "measures" : [
                    [
                      "avg",
                      "Opportunity.Opportunity_Age"
                    ]
                  ],
                  "filters" : [
                    [
                      "Opportunity.CloseDate",
                      [
                        [
                          [
                            "day",
                            -60
                          ],
                          [
                            "day",
                            -31
                          ]
                        ]
                      ],
                      ">=<="
                    ]
                  ]
                },
                "name" : "Prev30Days",
                "header" : "Prev30Days"
              },
              {
                "query" : {
                  "measures" : [
                    [
                      "count",
                      "*"
                    ]
                  ],
                  "formula" : "round((Last30Days - Prev30Days) / Prev30Days,2)"
                },
                "name" : "PercChangeVsPrev30Days",
                "header" : "PercChangeVsPrev30Days"
              },
              {
                "query" : {
                  "measures" : [
                    [
                      "count",
                      "*"
                    ]
                  ],
                  "formula" : "case when PercChangeVsPrev30Days <0 then \"#BA0517\" when PercChangeVsPrev30Days  >0 then \"#2e844a\" else \"#514f4d\" end"
                },
                "name" : "Color",
                "header" : "Color"
              }
            ]
