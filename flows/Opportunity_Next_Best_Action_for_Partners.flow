        </rules>
        <rules>
            <name>Proposal_Quote</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GetOpportunity.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Proposal/Quote</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Proposal_Quote_Stage</targetReference>
            </connector>
            <label>Proposal/Quote</label>
        </rules>
        <rules>
            <name>Negotiation</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>GetOpportunity.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Negotiation</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Negotiation_Stage</targetReference>
            </connector>
            <label>Negotiation</label>
        </rules>
