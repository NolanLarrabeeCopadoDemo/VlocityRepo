            <value>
                <elementReference>$Record.EndUser.ContactId</elementReference>
            </value>
        </filters>
        <object>Contact</object>
        <outputReference>RelatedContact</outputReference>
        <queriedFields>Id</queriedFields>
        <queriedFields>Name</queriedFields>
        <queriedFields>AccountId</queriedFields>
    </recordLookups>
