<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>54.0</apiVersion>
    <decisions>
        <name>Check_Messaging_User</name>
        <label>Check Messaging User</label>
        <locationX>182</locationX>
        <locationY>288</locationY>
        <defaultConnector>
            <targetReference>Phone_Number_Search</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Not using Messaging Users</defaultConnectorLabel>
        <rules>
            <name>User_Associated</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.EndUserId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Contact</targetReference>
            </connector>
            <label>User Associated</label>
        </rules>
    </decisions>
    <formulas>
        <name>CaseSubject</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Record.CallType}) + &apos;Support call&apos; +
IF(ISBLANK({!RelatedContact.Name}), &apos;&apos;, &apos; with &apos; + {!RelatedContact.Name})</expression>
    </formulas>
    <formulas>
        <name>NumberToSearchWith</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!$Record.CallType}) == &apos;Inbound&apos;, {!$Record.FromPhoneNumber}, {!$Record.ToPhoneNumber})</expression>
    </formulas>
    <interviewLabel>IDO Service - Voice Call On Create {!$Flow.CurrentDateTime}</interviewLabel>
    <label>IDO Service - Voice Call On Create</label>
    <processMetadataValues>
