    <label>Eniac Generate Program Component Forecast</label>
    <parameters>
        <dataType>Text</dataType>
        <isMultiValue>false</isMultiValue>
        <label>Manufacturing Program Id</label>
        <name>Manufacturing_Program_Id</name>
    </parameters>
    <parameters>
        <dataType>Text</dataType>
        <isMultiValue>false</isMultiValue>
        <label>Program Component Template Item Name</label>
        <name>Program_Component_Template_Item_Name</name>
    </parameters>
    <parameters>
        <dataType>Text</dataType>
        <isMultiValue>false</isMultiValue>
        <label>Program Variant Template Item Name</label>
        <name>Program_Variant_Template_Item_Name</name>
    </parameters>
    <processType>ProgramBasedBusiness</processType>
    <status>Active</status>
    <transforms>
        <expressionFields>
            <alias>ComponentForecastFactForecastedQuantity</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>IF ({IsProductComponentEffective} == 1 , IF ( {CalculatedProductComponentQuantity} &lt; {PRMMinimumEligibleQuantity}  , {PRMMinimumEligibleQuantity} , {CalculatedProductComponentQuantity} ) , 0 )</expression>
            <length>16</length>
        </expressionFields>
        <expressionFields>
            <alias>ComponentForecastFactIdentifiedUnitPrice</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>IF(ISNULL({PBEUnitPrice}), 0, {PBEUnitPrice} )</expression>
            <length>16</length>
        </expressionFields>
        <label>Calculate Product Component Forecast Quantity And Identified Unit Price</label>
        <name>Calculate_Product_Component_Forecast_Quantity_And_Identified_Unit_Price</name>
        <sourceName>Filter_Component_Forecast_Fact_Record_Outside_Program_Or_Period_Group_Date_Range</sourceName>
        <transformationType>Expression</transformationType>
    </transforms>
    <transforms>
        <expressionFields>
            <alias>ComponentForecastFactForecastedRevenue</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>{ComponentForecastFactForecastedQuantity} * {ComponentForecastFactIdentifiedUnitPrice}</expression>
            <length>16</length>
        </expressionFields>
        <expressionFields>
            <alias>ComponentForecastFactStatus</alias>
            <dataType>Text</dataType>
            <expression>&quot;Active&quot;</expression>
            <length>10</length>
        </expressionFields>
        <label>Calculate Product Component Forecast Revenue And Set Default Status</label>
        <name>Calculate_Product_Component_Forecast_Revenue_And_Set_Default_Status</name>
        <sourceName>Calculate_Product_Component_Forecast_Quantity_And_Identified_Unit_Price</sourceName>
        <transformationType>Expression</transformationType>
    </transforms>
    <transforms>
        <expressionFields>
            <alias>CalculatedAAFSUEndDate</alias>
            <dataType>Date</dataType>
            <expression>IF ({AAFPType} == &quot;Week&quot;, ADDDAYS({CurrentPeriodStartDate}, {EndPeriodNumber} * 7), ADDMONTHS({CurrentPeriodStartDate}, {EndPeriodNumber}))</expression>
        </expressionFields>
        <expressionFields>
            <alias>CalculatedAAFSUStartDate</alias>
            <dataType>Date</dataType>
            <expression>IF ({AAFPType} == &quot;Week&quot;, ADDDAYS({CurrentPeriodStartDate}, -7 * {StartPeriodNumber}), ADDMONTHS({CurrentPeriodStartDate}, -1 * {StartPeriodNumber}))</expression>
        </expressionFields>
        <label>Compute Account Forecast Set Use Adjusted Dates</label>
        <name>Compute_Account_Forecast_Set_Use_Adjusted_Dates</name>
        <sourceName>Compute_Account_Forecast_Set_Use_Date_Offsets</sourceName>
        <transformationType>Expression</transformationType>
    </transforms>
    <transforms>
        <expressionFields>
            <alias>CurrentPeriodStartDate</alias>
            <dataType>Date</dataType>
            <expression>{PeriodStartDate}</expression>
        </expressionFields>
        <expressionFields>
            <alias>EndPeriodNumber</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>0</decimalPlaces>
            <expression>{TodayDatePeriodOffset} * ({AAFPDisplayDuration} - (-1 * {AAFPGStartPeriod}) )</expression>
            <length>3</length>
        </expressionFields>
        <expressionFields>
            <alias>StartPeriodNumber</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>0</decimalPlaces>
            <expression>{TodayDatePeriodOffset} * (-1 * {AAFPGStartPeriod})</expression>
            <length>3</length>
        </expressionFields>
        <label>Compute Account Forecast Set Use Date Offsets</label>
        <name>Compute_Account_Forecast_Set_Use_Date_Offsets</name>
        <sourceName>Compute_Today_Date_Day_Period_Offset_For_Account_Forecast_Set_Use</sourceName>
        <transformationType>Expression</transformationType>
    </transforms>
    <transforms>
        <expressionFields>
            <alias>CartesianRightNodeAttrWithPeriodData</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>0</decimalPlaces>
            <expression>IF( {PeriodId} == {PeriodId} , 1 , 1)</expression>
            <length>1</length>
        </expressionFields>
        <expressionFields>
            <alias>FormattedPeriodStartDate</alias>
            <dataType>Date</dataType>
            <expression>ADDMONTHS( {PeriodStartDate} , 0)</expression>
        </expressionFields>
        <expressionFields>
            <alias>TodayDate</alias>
            <dataType>Date</dataType>
            <expression>TODAY()</expression>
        </expressionFields>
        <label>Compute Join Fields On Period Nodes</label>
        <name>Compute_Join_Fields_On_Period_Nodes</name>
        <sourceName>Period</sourceName>
        <transformationType>Expression</transformationType>
    </transforms>
    <transforms>
        <expressionFields>
            <alias>IsValidPeriodNodeForAAFSUStartDate</alias>
