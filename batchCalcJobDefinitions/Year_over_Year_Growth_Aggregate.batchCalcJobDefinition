    </transforms>
    <transforms>
        <description>Identifies new or updated records to be upserted into the Rebate Program Member Aggregate object.</description>
        <expressionFields>
            <alias>IsUpdateOrNew</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>1</decimalPlaces>
            <expression>IF(({TotalAggQuantity} == {TotalQuantity}) &amp;&amp; ({TotalAggAmount} == {TotalTransactionAmount}) &amp;&amp;  ({PriorAggQuantity} == {PriorTotalQuantity} ) &amp;&amp; ({PriorAggAmount} =={PriorTotalTransactionAmount}), 0,1)</expression>
            <length>2</length>
        </expressionFields>
        <label>Compute New Or Updated Aggregate Records</label>
