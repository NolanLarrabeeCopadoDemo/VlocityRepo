    <transforms>
        <description>Computes the growth metric on the aggregated data.</description>
        <expressionFields>
            <alias>GrowthAmount</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>{TotalAggAmount}-NULLVALUE({PriorAggAmount}, 0)</expression>
            <length>16</length>
        </expressionFields>
        <expressionFields>
            <alias>GrowthAmountPercent</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>IF({PriorAggAmount}&gt;0, ({TotalAggAmount} - {PriorAggAmount})*100/{PriorAggAmount},999999999999999)</expression>
            <length>16</length>
        </expressionFields>
        <expressionFields>
            <alias>GrowthQuantity</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>{TotalAggQuantity}-NULLVALUE({PriorAggQuantity}, 0)</expression>
            <length>16</length>
        </expressionFields>
        <expressionFields>
            <alias>GrowthQuantityPercent</alias>
            <dataType>Numeric</dataType>
            <decimalPlaces>2</decimalPlaces>
            <expression>IF({PriorAggQuantity}&gt;0, ({TotalAggQuantity} - {PriorAggQuantity})*100/{PriorAggQuantity},999999999999999)</expression>
            <length>16</length>
        </expressionFields>
        <label>Compute Growth Metrics on Aggregate Records</label>
